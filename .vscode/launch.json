    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "c:/Users/schaf.JANPC/OneDrive/_Coding/Gw2TP",
      "program": "c:/Users/schaf.JANPC/OneDrive/_Coding/Gw2TP/build/Debug/outDebug",
      "MIMode": "gdb",
